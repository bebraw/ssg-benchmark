{
  "name": "ssg-benchmark",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "wrangler pages dev .",
    "prebenchmark:all": "rm -rf ./benchmark-output",
    "benchmark:all": "tsx ./benchmark-suites/all.ts",
    "benchmark:scaling": "tsx ./benchmark-suites/scaling.ts",
    "benchmark:ssg": "tsx ./benchmark-suites/ssg.ts",
    "prebuild:cloudflare": "rm -rf ./dist",
    "build:cloudflare": "npm run build:vanilla",
    "build:all": "npm run build:breezewind && npm run build:11ty && npm run build:react && npm run build:vanilla",
    "build:breezewind": "tsx ./ssg/breezewind.ts",
    "build:11ty": "tsx ./ssg/11ty.ts",
    "build:react": "tsx ./ssg/react.ts",
    "build:vanilla": "tsx ./ssg/vanilla.ts",
    "serve:benchmark": "serve -p 8788 ./benchmark-output",
    "serve:static": "serve -p 8788 ./dist",
    "autocannon": "node ./run-autocannon.js",
    "autocannon:breezewind-on-edge": "autocannon -d=30 https://ssg-benchmark.pages.dev/breezewind-on-edge/posts/",
    "autocannon:edge": "autocannon -d=30 https://ssg-benchmark.pages.dev/edge/posts/",
    "autocannon:edge-with-isr": "autocannon -d=30 https://ssg-benchmark.pages.dev/edge-with-isr/posts/",
    "autocannon:netlify": "autocannon -d=30 https://ssg-benchmark.netlify.app/vanilla/posts/",
    "autocannon:ssg": "autocannon -d=30 https://ssg-benchmark.pages.dev/vanilla/posts/",
    "autocannon:local:breezewind-on-edge": "autocannon -d=30 http://127.0.0.1:8788/breezewind-on-edge/posts/",
    "autocannon:local:edge": "autocannon -d=30 http://127.0.0.1:8788/edge/posts/",
    "autocannon:local:edge-with-isr": "autocannon -d=30 http://127.0.0.1:8788/edge-with-isr/posts/",
    "autocannon:local:ssg": "autocannon -d=30 http://localhost:5000/vanilla/posts/",
    "playwright": "playwright test"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "breezewind": "^1.1.0",
    "mkdirp": "^1.0.4",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rmfr": "^2.0.0",
    "wrangler": "^2.7.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@playwright/test": "^1.29.2",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/rmfr": "^2.0.1",
    "autocannon": "^7.10.0",
    "benny": "^3.7.1",
    "glob": "^8.0.3",
    "lighthouse": "^9.6.8",
    "playwright": "^1.29.2",
    "playwright-lighthouse": "^2.2.2",
    "prettier": "^2.8.2",
    "serve": "^14.1.2",
    "tslib": "^2.4.1",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  }
}
