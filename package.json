{
  "name": "ssg-benchmark",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "wrangler pages dev .",
    "prebenchmark:all": "rm -rf ./benchmark-output",
    "benchmark:all": "tsx ./benchmark-suites/all.ts",
    "benchmark:scaling": "tsx ./benchmark-suites/scaling.ts",
    "benchmark:ssg": "tsx ./benchmark-suites/ssg.ts",
    "build:all": "npm run build:breezewind && npm run build:11ty && npm run build:react && npm run build:vanilla",
    "build:breezewind": "tsx ./ssg/breezewind.ts",
    "build:11ty": "tsx ./ssg/11ty.ts",
    "build:react": "tsx ./ssg/react.ts",
    "build:vanilla": "tsx ./ssg/vanilla.ts",
    "serve:benchmark": "serve -p 8788 ./benchmark-output",
    "serve:static": "serve -p 8788 ./dist",
    "test:breezewind-on-edge": "autocannon https://ssg-benchmark.pages.dev/breezewind-on-edge/posts/",
    "test:edge": "autocannon https://ssg-benchmark.pages.dev/edge/posts/",
    "test:ssg": "autocannon https://ssg-benchmark.pages.dev/ssg/posts/"
  },
  "keywords": [],
  "author": "Juho Vepsäläinen",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "breezewind": "^1.0.0",
    "mkdirp": "^1.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rmfr": "^2.0.0",
    "wrangler": "^2.0.25"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@playwright/test": "^1.25.0",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.7.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/rmfr": "^2.0.1",
    "autocannon": "^7.9.0",
    "benny": "^3.7.1",
    "prettier": "^2.7.1",
    "serve": "^14.0.1",
    "tslib": "^2.4.0",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4"
  }
}
